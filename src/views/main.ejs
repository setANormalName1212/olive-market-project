<%- include("partials/header") %> 

<%- include("partials/nav") %> 

<%- include("partials/messagess") %> 

<main class="container">
    <div class="row">
        <section class="col-md-5 d-flex justify-content-center">
            <a href="/product/add" class="nav-link">Add products</a>
    
    
            <% for(let i = 0; i < products.length; i++) { %>
                <article>
                    <a href="/product/get/<%= products[i].id %> ">
                        <h1><%= products[i].title %> </h1>
                    </a>
                </article>
            <% } %> 
        </section>
        
        <section class="col-md-7 d-flex justify-content-center">
    
            <a href="/supplier/add" class="nav-link">Add supplier</a>
    
            <% for(let i = 0; i < suppliers.length; i++) { %>
                <article>
                    <a href="/supplier/get/<%= suppliers.id %> ">
                        <h1><%= suppliers[i].name %> </h1>
                    </a>
                </article>
            <% } %>  
        </section>
    </div>
    <div>
        
    </div>
</main>

<%- include("partials/footer") %> 