<%- include("partials/header") %> 

<header class="bg-success p-4">
    <nav class="container">
        <a href="/register" class="">Register</a>
    </nav>
</header>

<%- include("partials/messagess") %> 

<section class="container p-2 d-flex justify-content-center">
    <form action="/admin/login" method="post" class="row w-25 p-4 btn btn-outline-success rounded mt-5">
        <label class="mt-3">
            <h3 class="text-start">Email</h3>
            <input type="email" name="email">
        </label>
        <label class="mt-3">
            <h3 class="text-start">Password</h3>
            <input type="password" name="password">
        </label>
        <button type="submit" class="mt-3 mb-2 btn btn-success">Log in</button>
    </form>
</section>

<%- include("partials/footer") %> 